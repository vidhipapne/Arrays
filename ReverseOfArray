public class Reverse {

	public static void main(String[] args) {
	   int a [] = {2,3,4,5,6,8};
	   int l = a.length;
	   System.out.println(l);
	   System.out.println();
	   
       int b [] = new int [l];
       
       for(int i=0;i<l;i++) {
    	  b[i]= a[l-i-1];
       }
       
       for(int i=0;i<b.length;i++) {
       System.out.println(b[i]);
   	}
       
	}

}
