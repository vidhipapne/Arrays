
public class Duplicate {

	public static void main(String[] args) {
         int a[] = { 2,4,5,6,6,7,8,2,1,1,3,2};
         
         for(int i=0;i<a.length;i++) {
        	 for(int j=i+1;j<a.length;j++) {
        		 if(a[i]==a[j]) {
        			 System.out.println("Duplicate: "+ a[j]);
        			 System.out.println("Position: "+ j);
        			 System.out.println();
        			 break;
        		 }
        	 }
         }

	}

}

