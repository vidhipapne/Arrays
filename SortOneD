
public class SortOneD {

	public static void main(String[] args) {
		int a [] = {9,2,1,3,4,7,6,5};
		
		for(int i=0; i<a.length; i++)
	    {
	        for(int j=i+1; j<a.length; j++)
	        {
	            if(a[j] <a[i])
	            {
	             int temp = a[i];
	                a[i] = a[j];
	                a[j] = temp;
	            }
	        }
	    }
		
		for(int i1=0;i1<a.length;i1++) {
		System.out.println( a[i1]);
		}
	   
	}
}


